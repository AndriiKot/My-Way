@import './styl/variables.styl'

border-radius(radius = 5px)
    --webkit-border-radius radius
    border-radius radius

cf() 
    zoom 1
    &:after
    &:before
        content ""
        display:  table
    &:after
        clear both

body 
    background var_blue
    color var_red
    text-align center
    border-radius(20px)
    h1
        color @color
        border solid 1px @color
        border-radius(20px)
        cf()


            
            

    